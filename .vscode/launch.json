	"version": "0.2.0",
	"configurations": [

		{
			"name": "(lldb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program":"${workspaceFolder}/pipex/pipex",
			"args": [
				"dup.c ", "grep s " ,"cat","sort", "ls" ," test1vscode"
			],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug",
			"program": "${workspaceFolder}/pipex/pipex",
			"args": [
				"ss", "grep s " ,"cat" ," sss"
			],
			"cwd": "${workspaceFolder}"
		}
	]
}