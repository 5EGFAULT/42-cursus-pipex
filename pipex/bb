		exit(errno);$
		perror("");$
	//int writefdd = dup2(1, 1);$
	fdin = open("in", O_RDONLY);$
	fdout= open("out", O_WRONLY | O_TRUNC | O_CREAT);$
	if (fdin < 0 || fdout <0 || pipe(p1) < 0 || pipe(p2) < 0)$
	int readfdd = dup2(fdin, 0);$
	int strl = read(0, str, 200);$
	return (0);$
